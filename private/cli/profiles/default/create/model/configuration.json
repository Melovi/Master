{
  "variables" : [
    {
      "name" : "modelName",
      "description" : "What's your model called (Mongo.Collection)?"
    }
  ],
  "files" : [
    {
      "template" : "model.js",
      "path" : "models/__modelName__.js"
    },
    {
      "template" : "subscription.js",
      "path" : "client/subscriptions/__modelName__Sub.js"
    },
    {
      "template" : "publication.js",
      "path" : "server/publications/__modelName__Pub.js"
    }
  ]
}